{
  "nats": {
    "servers": ["nats://localhost:4222"],
    "name": "soxdrawer-js-server",
    "timeout": 10000,
    "pingInterval": 20000,
    "maxPingOut": 2
  },
  "http": {
    "port": 8080,
    "host": "localhost"
  },
  "objectStore": {
    "defaultBucket": "default",
    "description": "SoxDrawer object storage",
    "storage": "file",
    "replicas": 1
  },
  "logging": {
    "level": "info",
    "format": "json"
  }
}
